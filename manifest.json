{
  "manifest_version": 3,
  "name": "Disney Dates Grabber",
  "version": "1.0.0",
  "description": "Logs resort slug + check-in/out dates from disneyworld.com SPA.",
  "permissions": [],
  "host_permissions": [
    "https://disneyworld.disney.go.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://disneyworld.disney.go.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
